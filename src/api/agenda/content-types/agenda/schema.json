{
  "kind": "collectionType",
  "collectionName": "agendas",
  "info": {
    "singularName": "agenda",
    "pluralName": "agendas",
    "displayName": "Agenda"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "content": {
      "type": "blocks"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category_agenda": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category-agenda.category-agenda"
    },
    "dateStart": {
      "type": "date",
      "required": true
    },
    "dateEnd": {
      "type": "date"
    },
    "location": {
      "type": "string",
      "required": true
    },
    "linkRegister": {
      "type": "string"
    }
  }
}
